    "to-camel-case": "^1.0.0",
    "to-pascal-case": "^1.0.0",
    "underscore": "^1.13.4",
    "util": "^0.12.4",
    "uuidv4": "^6.2.13",
    "validator": "^13.0.0",
    "winston": "^3.2.1",
    "xss-clean": "^0.1.1"
